CREATE TABLE `borabora`.`spa` (`num_soin` INT NOT NULL AUTO_INCREMENT , `lib_soin` VARCHAR(50) NOT NULL , `prix_soin` INT NOT NULL , `desc_soin` TEXT NULL , PRIMARY KEY (`num_soin`)) ENGINE = MyISAM;



Ajouter durée
ALTER TABLE `spa` ADD `duree` INT NOT NULL AFTER `lib_soin`;


